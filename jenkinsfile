pipeline {
    agent any
    
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'
            }
        }

        stage('build') {
            steps {
                echo 'Entering build stage'
                bat 'mvn -v'
                bat 'mvn clean package'
                bat 'java -jar  \target\school-0.0.1-SNAPSHOT.jar'
            }
        }
    }
}